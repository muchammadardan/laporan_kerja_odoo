<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <!-- Add you code here -->

        <!-- GROUP -->
        <record model="ir.module.category" id="kf_lap_teknis_group">
            <field name="name">Kimia Farma</field>
            <field name="description">This group is for parent other group</field>
            <field name="sequence">10</field>
        </record>

        <!-- USER -->
        <record model="res.groups" id="user">
            <field name="name">User</field>
            <field name="category_id" ref="kf_lap_teknis_group"/>
        </record>

        <!-- ADMIN -->
        <record model="res.groups" id="admin">
            <field name="name">Admin</field>
            <field name="category_id" ref="kf_lap_teknis_group"/>
            <field name="implied_ids" eval="[
                (4, ref('user'))
            ]"/>
        </record>

    </data>
</odoo>